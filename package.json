{
  "name": "@qc/react-video",
  "version": "0.0.1-alpha",
  "description": "A declarative HTML5 video React component.",
  "keywords": [
    "react",
    "video"
  ],
  "author": "Danny Hurlburt",
  "license": "ISC",
  "browserslist": [
    "> 0.5%",
    "Last 2 major versions",
    "Firefox ESR"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "files": [
    "lib/",
    "es/",
    "src/"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=cjs babel src -d lib/",
    "build:es": "cross-env BABEL_ENV=es babel src -d es/",
    "clean": "rimraf es/ lib/",
    "lint": "eslint src/",
    "prepublishOnly": "npm run clean && npm run lint && npm run build",
    "postpublish": "npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-remove-debugger": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "react": "^16.2.0"
  }
}
